#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){ 
            cin>>a[i];
        }
        int count=0;  
        int min=a[n-1]; 
        for(int i=n-2;i>=0;i--){ // ulti a pri
            if(a[i]>min){ 
                count++;
            }
            else{
                min=a[i];
            }
        }
        cout<<count<<endl;
    }
    return 0;
}
