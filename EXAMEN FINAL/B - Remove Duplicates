 
#include <iostream>
#include <cstdio>
#include <cstring>
#include <algorithm>
#include <cmath>
 
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++) //input the array
    {
        cin>>a[i];
    }
    int count=0;
 
    for(int i=0;i<n;i++) //print the unique elements
    {
        for (int j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                a[i]=0;
            }
        }
    }
 
    for(int i=0;i<n;i++) //count the unique elements
    {
        if(a[i]!=0)
        {
            count++;
        }
    }
    cout<<count<<endl;
    for(int i=0;i<n;i++)
    {
        if(a[i]!=0)
        {
            cout<<a[i]<<" ";
        }
    }
 
 
    
    return 0;
}
